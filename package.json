{
	"name": "emacsplus",
	"displayName": "emacsplus",
	"description": "An Emacs key bindings for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:emacsplus.moveBeginingOfLine",
		"onCommand:emacsplus.moveEndOfLine"

	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emacsplus.moveBeginingOfLine",
				"title": "Move Begining Of Line"
			},
			{
				"command": "emacsplus.moveEndOfLine",
				"title": "Move End Of Line"
			}
		],
		"keybindings": [
			{
				"win": "ctrl+a",
				"mac": "ctrl+a",
				"linux": "ctrl+a",
				"key": "ctrl+a",
				"when": "editorTextFocus",
				"command": "emacsplus.moveBeginingOfLine"
			},
			{
				"win": "ctrl+e",
				"mac": "ctrl+e",
				"linux": "ctrl+e",
				"key": "ctrl+e",
				"when": "editorTextFocus",
				"command": "emacsplus.moveEndOfLine"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
